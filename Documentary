#include <iostream>
using namespace std;

int main() {
    int N;
    
    // Input the value of N
    cout << "Enter a number N: ";
    cin >> N;
    
    // Calculate the sum using the formula
    int sum = N * (N + 1) / 2;
    
    // Output the sum
    cout << "Sum of natural numbers up to " << N << " is: " << sum << endl;
    
    return 0;
}
